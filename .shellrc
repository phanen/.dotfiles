# run sh2fish.sh

[[ -f ~/.config/shell/alias.sh ]] && . ~/.config/shell/alias.sh
[[ -f ~/.config/shell/func.sh ]] && . ~/.config/shell/func.sh
[[ -f ~/.config/shell/pkg.sh ]] && . ~/.config/shell/pkg.sh

# disable tty ctrl-s
[[ "$(ps -o comm= -p $$)" != fish ]] && stty stop undef

case  $(tty) in
  *tty*) setfont ter-d28b;;
  *) ;;
esac
